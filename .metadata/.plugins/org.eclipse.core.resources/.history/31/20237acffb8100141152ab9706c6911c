package priorityQueue;

public class Key implements Comparable<Key>{
	private int value;
	
	//Constructor
	public Key(int in){
		value=in;
	}
	
	public void setValue(int x){
		value=x;
	}
	
	public int getValue(){
		return value;
	}
	@Override
	public int compareTo(Key k) {
		int v = value - k.getValue();
		if(v < 0)		return -1;
		else if(v > 0)  return 1;
		else			return 0;
	}
}
