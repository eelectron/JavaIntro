package priorityQueue;

public class Key implements Comparable{
	private int value;
	
	//Constructor
	public Key(int in){
		value=in;
	}
	
	public void setValue(int x){
		value=x;
	}
	
	public float getValue(){
		return value;
	}
	@Override
	public int compareTo(Object o) {
		Key k=(Key)o;
		float v = value - k.getValue();
		
		if(v < 0)		return -1;
		else if(v > 0)  return 1;
		else			return 0;
	}
}
