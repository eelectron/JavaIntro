package bst;

import java.util.Scanner;

public class BSTTestClient {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		BST bst = new BST<Key>();
		Scanner sc=new Scanner(System.in);
		
		//Used to form Key
		Key key;
		long number;
		
		//Used in Value class
		Value v;
		String username;
		System.out.println("Enter a integer and a username:");
		while(sc.hasNextLine()){
			System.out.println("Enter a integer and a username:");
			//take line split it into tokens
			String[] tokens = sc.nextLine().split(" ");
			
			//Form Key object
			number = Long.parseLong(tokens[0]);
			key=new Key(number);
			
			//Form Value object
			username=tokens[1];
			v = new Value(username);
			
			//Put in bst
			bst.put(key, v);
			
			//Get from bst
			System.out.println(bst.get(key));
		}
	}

}
