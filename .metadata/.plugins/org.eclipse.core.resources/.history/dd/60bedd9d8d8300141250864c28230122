package bst;

public class BST23Tree {
	Node root;
	
	//Node-contains link atmost to 3 other node
	private class Node{
		private Key k1,k2;
		private Value v1,v2;
		private Node left,middle,right;
		public Node(Key k, Value v){
			k1=k; v1=v;
		}
	}
	
	//Inserts a key
	public void put(Key k, Value v){
		if (root == null) {
			root=new Node(k,v);
		}else if(root.k2 == null && (k.compareTo(root.k1) > 0) ){
			root.k2=k;
			root.v2 = v;
		}
	}
}
