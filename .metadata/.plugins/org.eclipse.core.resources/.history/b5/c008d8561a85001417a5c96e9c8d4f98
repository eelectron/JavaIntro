package graph;

import java.util.LinkedList;

public class Graph {
	private final int noOfVertex;
	private LinkedList<Integer>[] adj;
	public Graph(int V){
		this.noOfVertex=V;
		adj = new LinkedList[noOfVertex];
		for (int v = 0; v < noOfVertex; v++) {
			adj[v]=new LinkedList<Integer>();
		}
	}
}
