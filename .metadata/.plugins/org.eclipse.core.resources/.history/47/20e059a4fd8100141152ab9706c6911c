package bst;

public class Key implements Comparable<Key>{
	private float keyValue;
	
	//Constructor
	public Key(float in){
		keyValue=in;
	}
	
	public void setValue(float x){
		keyValue=x;
	}
	
	public float getValue(){
		return keyValue;
	}
	@Override
	public int compareTo(Key k) {
		float v = keyValue - k.getValue();
		if(v < 0)		return -1;
		else if(v > 0)  return 1;
		else			return 0;
	}
}
